<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="context" prefHeight="191.0" prefWidth="322.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.developersstack.edumanage.controller.ResetPasswordFormController">
   <children>
      <Label layoutX="34.0" layoutY="25.0" text="Reset Password" textFill="#000fff">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button layoutX="34.0" layoutY="128.0" mnemonicParsing="false" onAction="#changePasswordOnAction" prefHeight="29.0" prefWidth="253.0" style="-fx-background-color: #18bdff;" text="Change">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <PasswordField fx:id="txtPassword" layoutX="33.0" layoutY="83.0" prefHeight="25.0" prefWidth="253.0" promptText="New Password">
         <font>
            <Font size="15.0" />
         </font>
      </PasswordField>
   </children>
</AnchorPane>
